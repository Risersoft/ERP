<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://erp.qa.appsnirvana.com</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>http://erp.qa.appsnirvana.com</MSDeployServiceURL>
    <DeployIisAppPath>erpv2</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>RemoteAgent</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>qa.test</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="risersoft.app.mxform.gst.mxgstEntities" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="risersoft.app.mxform.gst.mxgstEntities, risersoft.app.mxform.gst" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="risersoft.app.mxform.hrm.mxentDbEntities" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="risersoft.app.mxform.hrm.mxentDbEntities, risersoft.app.mxform.hrm" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)risersoft.app.mxform.gst.mxgstEntities-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)risersoft.app.mxform.hrm.mxentDbEntities-Web.config Connection String" />
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="risersoft.app.mxform.gst.mxgstEntities" />
    <_ConnectionStringsToInsert Include="risersoft.app.mxform.hrm.mxentDbEntities" />
  </ItemGroup>
</Project>